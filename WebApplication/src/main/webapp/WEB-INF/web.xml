<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>WebApplication</display-name>
<servlet>
<servlet-name>homeSvlt</servlet-name>
<servlet-class>com.janadri.MyServlet</servlet-class>

<init-param>
<param-name>name</param-name>
<param-value>Janadri</param-value>
</init-param>
</servlet>


<servlet-mapping>
<servlet-name>homeSvlt</servlet-name>
<url-pattern>/home</url-pattern>
</servlet-mapping>

<context-param>
<param-name>name</param-name>
<param-value>Veeru</param-value>
</context-param>

<context-param>
<param-name>Phone</param-name>
<param-value>Apple</param-value>
</context-param>
</web-app>